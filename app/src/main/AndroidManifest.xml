<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.oa10">

    <!-- 读取联系人权限 -->
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <!-- 拨打电话权限 -->
    <uses-permission android:name="android.permission.CALL_PHONE"/>
    <!--修改SD卡权限-->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/we"
        android:label="@string/app_name"
        android:roundIcon="@drawable/we"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".Activity.PhoneContentActivity">
        </activity>
        <activity android:name=".Activity.LogNewBuild">
        </activity>
        <activity android:name=".Activity.InformActivity">
        </activity>
        <activity android:name=".Activity.LogActivity">
        </activity>
        <activity android:name=".Activity.LoginActivity">
        </activity>
        <activity android:name=".Activity.DetailActivity">
        </activity>
        <activity android:name=".Activity.StickyActivity">
        </activity>
        <activity android:name=".Activity.StickyNewBuild">
        </activity>
    </application>

</manifest>